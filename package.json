{
  "name": "sistema-marcos-geodesicos",
  "version": "2.0.0",
  "description": "Sistema de gestão de marcos geodésicos com GIS integrado",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "start:postgres": "node backend/server-postgres.js",
    "dev": "nodemon backend/server.js",
    "dev:postgres": "nodemon backend/server-postgres.js",
    "migrate": "node backend/migrate.js migrate",
    "migrate:status": "node backend/migrate.js status",
    "migrate:verify": "node backend/migrate.js verify",
    "migrate:pg": "node backend/scripts/migrar-completo.js",
    "validate:pg": "node backend/scripts/validar-migracao.js",
    "test:api": "node backend/scripts/testar-api-postgres.js",
    "test:clientes-propriedades": "node backend/scripts/test-api.js",
    "cleanup": "bash scripts/cleanup-old-system.sh",
    "db:reset": "node backend/reset-database.js",
    "db:clean": "node backend/clean-migrations.js",
    "debug": "node playwright-debug-config.js",
    "debug:headless": "set HEADLESS=true && node playwright-debug-config.js",
    "test:premium": "node test-premium.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "geodesia",
    "marcos",
    "gis",
    "cogep"
  ],
  "author": "COGEP",
  "license": "ISC",
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "better-sqlite3": "^9.2.2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dbf": "^0.2.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "geojson-precision": "^1.0.0",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "playwright": "^1.56.1",
    "proj4": "^2.19.10",
    "shapefile": "^0.6.6",
    "supercluster": "^8.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "nodemon": "^3.0.2"
  }
}
